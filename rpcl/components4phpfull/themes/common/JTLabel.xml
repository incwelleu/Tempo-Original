<skins>
  <!-- The default skin for this label theme -->
  <skin id="default">
    <!-- The HTML code for the label -->
    <code id="label">
      <!--
      Available variables:
      {$NAME}:            Name of component.
      {$WIDTH}:           Width of component.
      {$HEIGHT}:          Height of component.
      {$TEXTCLASS}:       Text class selected by the user for the label font.
      {$STYLE}:           CSS font overrides for the label.
	  {$CSSCLASS}: 		  Custom CSS class.
      {$CONTENT}:         Label caption. May contain link code, if the user has specified a link for this label.
      {$TABINDEX}:        Tab index override.
      -->
      <![CDATA[
<div id="{$NAME}" class="{$TEXTCLASS} {$CSSCLASS}" style="cursor: {$CURSOR}; overflow: hidden; width:{$WIDTH}; height:{$HEIGHT};{$STYLE}">{$CONTENT}</div>
      ]]>
    </code>
    <code id="link">
      <!--
      Available variables:
      {$NAME}:            Name of component.
      {$WIDTH}:           Width of component.
      {$HEIGHT}:          Height of component.
      {$LINK}:            Destination URL.
      {$CONTENT):         Label caption.
      {$TABINDEX}:        Tab index override.
      -->
      <![CDATA[
<a id="{$NAME}_link" href="{$LINK}" tabindex="{$TABINDEX}">{$CONTENT}</a>
      ]]>
    </code>
  </skin>
</skins>